{
  "name": "eginkaman/aba-file-generator",
  "description": "Generate ABA bank transaction files from a collection of transactions.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "EginKaman",
      "email": "egin.kaman@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "laravel/framework": "^10.8"
  },
  "autoload": {
    "psr-4": {
      "EginKaman\\AbaFileGenerator\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "EginKaman\\AbaFileGenerator\\Providers\\AbaFileGeneratorServiceProvider"
      ],
      "aliases": {
        "AbaFileGenerator": "EginKaman\\AbaFileGenerator\\Facades\\AbaFileGenerator"
      }
    }
  }
}
